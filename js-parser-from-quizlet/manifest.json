{
    "manifest_version": 2,
    "name": "Parse some values from the quizlet for the bot",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "https://quizlet.com/*"
        ],
        "js": ["content.js"]
      }
    ],
    "permissions": [
        "http://www.yourwebsite.com/"
    ]
}